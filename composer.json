{
  "name": "flatten/test",
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "friendsofphp/php-cs-fixer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Src\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Src\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.0"
    },
    "sort-packages": true
  }
}